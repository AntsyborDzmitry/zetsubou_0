        jcr:primaryType="sling:MessageEntry"
        sling:message="Office"/>
	<phone__mobile_option
        jcr:primaryType="sling:MessageEntry"
        sling:message="Mobile"/>
	<phone__other_option
        jcr:primaryType="sling:MessageEntry"
        sling:message="Other"/>
	<phone__country_code_label
        jcr:primaryType="sling:MessageEntry"
        sling:message="Country Code"/>
	<phone__extension_label
        jcr:primaryType="sling:MessageEntry"
        sling:message="Extension"/>
	<phone__SMS_enabled_label
        jcr:primaryType="sling:MessageEntry"
        sling:message="This is an SMS enabled phone"/>
   	<phone__fax_label
        jcr:primaryType="sling:MessageEntry"
        sling:message="Fax"/>
   	<phone__phone_type_label
        jcr:primaryType="sling:MessageEntry"
        sling:message="Phone Type"/>
    <phone__label
        jcr:primaryType="sling:MessageEntry"
