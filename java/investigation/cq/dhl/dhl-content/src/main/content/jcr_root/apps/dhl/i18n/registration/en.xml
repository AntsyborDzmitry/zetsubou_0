<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:language="en"
          jcr:mixinTypes="[mix:language]"
          jcr:primaryType="sling:Folder"
          sling:basename="[dhl,registration]">
    <account_add_one_or_more_accounts
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="question_is_user_has_dhl_account ((Question about existence of DHL account for user))"
        sling:message="Enter one or more DHL account numbers"/>
    <account_part_continue_button
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Continue"/>
    <account_tips_1
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Whether you have a DHL account or not, registering brings a host of time-saving features that make shipping online fast and easy."/>
    <account_tips_2
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="You can enter any of your company's export or import accounts and decide which account is your primary number when you login to MyDHL. You can also add more accounts later or change your primary in your profile."/>
    <add_account_field_button
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Add another account"/>
    <change_country_modal_description
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="This will refresh the current page and reset your DHL country preferences."/>
    <change_country_modal_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Change country"/>
    <change_country_modal_no_button_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Close without saving"/>
    <change_country_modal_title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Change country"/>
    <change_country_modal_yes_button_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Update"/>
    <country_code_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Country Code"/>
    <country_code_format_error
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Invalid Country code format"/>
    <company_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Company"/>
    <create_password_block_title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Login Details"/>
    <dhl_account_block_description
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="If your company will be shipping regularly, having an account offers many benefits. Preferential rates, regular billing, shipment monitoring and much more!"/>
    <dhl_account_request_checkbox_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Yes, I'd like to apply for a DHL account"/>
    <dhl_account_shipment_address_1_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Address"/>
    <dhl_account_shipment_address_2_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Address 2"/>
    <dhl_account_shipment_address_city_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="City"/>
    <dhl_account_shipment_address_state_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Province"/>
    <dhl_account_shipment_address_zip_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="ZIP"/>
    <dhl_account_shipment_frequency_select_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="How often do you plan to ship?"/>
    <email_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Email Address"/>
    <ext_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Extension"/>
    <first_name_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="First Name"/>
    <first_section_title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Register to Enjoy Time Saving Benefits"/>
    <general_error_validation
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Some errors on the form"/>
    <has_account_button_description
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Register with your account to view your preferential rates and bill shipments to your DHL account."/>
    <last_name_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Last Name"/>
    <make_account_primary_checkbox_title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Make primary"/>
    <marketing_acceptance_checkbox_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="I would like to receive informational emails and promotional offers from DHL"/>
    <no_account_button_description
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="No account? No problem! Register and pay by credit card or PayPal."/>
    <password_input_title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Password"/>
    <phone_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Primary Phone"/>
    <privacy_cookie_policy_checkbox_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="I accept the DHL Express"/>
    <privacy_cookie_policy_checkbox_label_err_msg
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="You should accept Privacy Policy to be able to register in eWF"/>
    <re_enter_password_field_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Confirm Password"/>
    <register_dhl_account_block_title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Would you like to apply for a DHL account?"/>
    <account_numbers_required
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Account number is required for registration"/>
    <account_only_alphanumeric_length_10
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Account should be alphanumeric, length of 10"/>
    <account_only_digits_length_5
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="5 digits"/>
    <account_only_digits_length_9
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="should have 9 digits"/>
    <continue
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Continue"/>
    <enter_account_number_title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Enter any DHL Account Number associated with your personal contact information"/>
    <error_no_such_account
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="There is no such account!"/>
    <has_dhl_account_no_answer
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="has_dhl_account_no_answer ((No answer in check box (User does not has DHL account)))"
        sling:message="No"/>
    <has_dhl_account_yes_answer
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="has_dhl_account_yes_answer ((Yes answer in checkbox))"
        sling:message="Yes"/>
    <head
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="head ((header for registration))"
        sling:message="Register Now"/>
    <password_capital_letter
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="contains at least one capital letter"/>
    <password_length_8
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="has length at least 8 chars"/>
    <password_length_8_35
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="has length from 8 to 35 chars"/>
    <password_number
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="contains at least one number"/>
    <password_small_letter
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="contains at least one non-capital letter"/>
    <primary_phone_only_digits_length_10
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Phone consists of 10 digits"/>
    <profile_details_nectar_points_should_be_numeric_value
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Nectar points should be numeric value"/>
    <primary_phone_only_digits_length_5
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Field is up to 5 digits"/>
    <question_is_user_has_dhl_account
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="question_is_user_has_dhl_account ((Question about existence of DHL account for user))"
        sling:message="Do you have a DHL account number?"/>
    <reguired_message
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Invalid entry. Mandatory fields are empty. Please fill in the fields, marked with *"/>
    <special_character
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="must contains special character"/>
    <remove_account_field_button
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Remove"/>
    <request_account_link_to_details
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="More details about benefits"/>
    <required_message
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Email is required"/>
    <rendering_in_progress
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="You will see page in a moment"/>
    <sms_enabled_checkbox_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="This is an SMS (text) enabled phoneIf you wish to receive shipment notifications while shipping, an SMS (text) enabled phone is required"/>
    <submit_button
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Register"/>
    <term_conditions_section_title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Complete to continue registration"/>
    <terms_and_conditions_checkbox_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="I accept the DHL Express"/>
    <terms_and_conditions_checkbox_label_err_msg
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="You should accept Terms and Conditions to be able to register in eWF"/>
    <title_input_label
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Title"/>
    <validation_email_already_exists
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Entered email is already registered. Please login. Or use another email for registration"/>
    <validation_email_format_invalid
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Email format is incorrect"/>
    <validation_latin_letter
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Latin letters only"/>
    <validation_password_match
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Entered and confirm Password doesn't match"/>
    <your_info_section_title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Your Contact Details"/>
    <validation_error_message
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Invalid data"/>
    <terms_and_conditions
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Terms and Conditions"/>
    <privacy_and_cookies_policy
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:message="Privacy and Cookies Policy"/>
    <email_address_verification_title
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Email address verification"/>
    <email_address_verification_success_msg
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Your email has been verified, welcome to eWF! You may log in"/>
    <email_address_verification_already_used_title
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Activation email is already used"/>
    <email_address_verification_expired_title
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="MyDHL Activation Email Expired"/>
    <email_address_verification_expired_msg_first_line
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Thank you for registering to eWF"/>
    <email_address_verification_expired_msg_second_line
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Unfortunately, the activation email has expired. Please click the below to generate a new activation email before logging into MyDHL."/>
    <email_address_verification_expired_renew_btn
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Send new activation email"/>
    <registration_success_status
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Done!"/>
    <registration_success_title
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="We've sent you a welcome email"/>
    <registration_success_message
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Just click the link to login and start using MyDHL!"/>
    <registration_account_holder_success_title
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Thank you for registering to eWF"/>
    <registration_account_holder_success_message_part1
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="There is still one more step to complete your registration. An activation email is on its way to your inbox and your registration will need to be activated within"/>
    <registration_account_holder_success_message_part2
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="day. Please be sure to check your inbox. Once you activate your registration, you can login and begin using MyDHL"/>
    <registration_account_holder_success_message
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="There is just one more step to complete your registration. An activation email is on its way to your inbox and your registration needs to be activated within 1 day. Please be sure to check your inbox. Once you activate your registration, you can login and begin using MyDHL."/>
    <registration_success_btn_text
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Continue"/>
    <registration_check_email_title
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Check your email"/>
    <registration_check_email_part3
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Once you activate your registration, you can login and begin using MyDHL"/>
    <registration_check_btn_text
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Continue"/>
    <registration_check_status
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:message="Almost Done!"/>

</jcr:root>
