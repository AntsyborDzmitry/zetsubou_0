{{#extend "scss"}}
{{#content "sprites"}}
{{#each sprites}}
${{strings.name}}: ( {{px.offset_x}}, {{px.offset_y}}, '{{name}}' );
{{/each}}
{{/content}}
{{#content "spritesheet"}}
$flags-{{spritesheet_info.name}}: ({{#each sprites}}${{strings.name}}, {{/each}});
$flag-source-{{spritesheet_info.name}}: '{{{spritesheet.escaped_image}}}';
{{/content}}
{{/extend}}