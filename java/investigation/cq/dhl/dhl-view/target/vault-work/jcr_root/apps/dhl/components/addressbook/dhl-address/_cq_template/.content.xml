<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <addressbook
          jcr:primaryType="nt:unstructured"
          sling:resourceType="dhl/components/addressbook/dhl-address-book"
          addressBookAttributes="exampleAttributes='exampleValue' some-other-attr">
        <search
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/form/dhl-search"
                searchAttributes="on-search='addressBookCtrl.handleSearch($data)'"
                showAddressBook="{Boolean}true"
                showAhead="{Boolean}true"
                showCategory="{Boolean}true"
                showSearchButton="{Boolean}true"/>
    </addressbook>
</jcr:root>