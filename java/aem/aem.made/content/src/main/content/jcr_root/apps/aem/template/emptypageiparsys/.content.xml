<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:description="Empty Page template with iparsys"
    jcr:primaryType="cq:Template"
    jcr:title="AEM Empty Page (ingerit)"
    allowedPaths="[/content(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        cq:designPath="/etc/designs/aem"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="aem/components/page/emptypageinherit">
            <container-inherited
                    sling:resourceType="foundation/components/iparsys"
                    jcr:primaryType="nt:unstructured"/>
    </jcr:content>
</jcr:root>
