<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
        jcr:description="Category Page Template"
        jcr:primaryType="cq:Template"
        jcr:title="Category Page"
        allowedPaths="[/content/training/.+]"
        ranking="{Long}100">
    <jcr:content
            jcr:primaryType="cq:PageContent"
            sling:resourceType="aem-training/components/pages/category-page">
        <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys">
            <category
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aem-training/components/content/category">
                <category-container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys"/>
            </category>
        </container>
    </jcr:content>
</jcr:root>
